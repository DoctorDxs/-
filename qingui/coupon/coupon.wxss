.qingful-coupon{
	height:230rpx;
	width:100%;
    background:#fff;
    color:#fff;
}
.qingful-coupon-content{
	display:inline-flex;
	flex-direction:row;
	justify-content:space-between;

}
.qingful-coupon-content-view{
	width:274rpx;
	height:146rpx;
	margin-right:10rpx;
	display:inline-flex;
	margin-top:42rpx;
	position:relative;
}
.qingful-coupon-left{
	width:210rpx;
	background:#fe5f3a;
	height:146rpx;
	border-top-left-radius:6rpx;
	border-bottom-left-radius:6rpx;
	border-right:1rpx solid rgba(254,135,71,0.5);
	display:none;

}
.qingful-coupon-right{
	width:64rpx;
	background:#fe5f3a;
	float:left;
	height:146rpx;
	border-top-right-radius:6rpx;
	border-bottom-right-radius:6rpx;
	font-size:20rpx;
	display:none
}
.qingful-coupon-right text{
	display:block;
	text-align:center;
	height:28rpx;
}
.qingful-coupon-button{
	position:absolute;
	background:red;
	width:16rpx;
	height:16rpx;
	left:202rpx;
	border-radius:16rpx;
	background:#fff;
	margin-top:-8rpx;
}
.qingful-coupon-bottom{
	position:absolute;
	background:red;
	width:16rpx;
	height:16rpx;
	left:202rpx;
	bottom:0rpx;
	border-radius:16rpx;
	background:#fff;
	margin-bottom:-8rpx;
}


